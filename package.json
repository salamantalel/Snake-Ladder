{
  "name": "snake-ladder",
  "version": "1.0.0",
  "private": true,
  "description": "A collaborative Snake and Ladder game built with React and JSON Server",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
